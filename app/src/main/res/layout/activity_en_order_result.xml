<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="16"
    android:orientation="vertical">
    <include
        android:id="@+id/order_result_title"
        layout="@layout/e_title"
        style="@style/TitleStyle"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        style="@style/body_wigth">
    <TextView
        android:id="@+id/weblink_hint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="查看网页"
        android:padding="@dimen/left_right_retract_small"
        android:textColor="@color/text_hei"
        android:textSize="14dp"/>
    <TextView
        android:id="@+id/order_resule_link"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/left_right_retract_small"
        android:autoLink="all"
        android:text="www.baidu.com"/>
    <TextView
        android:id="@+id/kanimage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/left_right_retract_small"
        android:text="查看截图"/>
    <ImageView
        android:id="@+id/order_resule_pic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_margin="@dimen/left_right_retract_small"
        android:scaleType="centerInside"/>
    </LinearLayout>
</LinearLayout>
