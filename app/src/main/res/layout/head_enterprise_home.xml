                    android:layout_weight="1">
                    <ImageView
                        android:id="@+id/e_home_reporter_image"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="18dp"
                        android:src="@mipmap/ic_launcher"/>
                    <TextView
                        android:id="@+id/e_home_reporter_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/text_hei"
                        android:layout_toRightOf="@+id/e_home_reporter_image"
                        android:layout_marginLeft="20dp"
                        android:textSize="14dp"
                        android:text="@string/e_home_reporter_text_vstr"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#c6c6c6"
                        android:layout_toRightOf="@+id/e_home_reporter_image"
                        android:layout_below="@+id/e_home_reporter_text"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="4dp"
