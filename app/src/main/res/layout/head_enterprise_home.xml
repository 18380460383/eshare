                </RelativeLayout>
                <TextView
                    android:layout_width="1dp"
                    android:layout_height="40dp"
                    android:background="#dedede"/>
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <ImageView
                        android:id="@+id/e_home_friend_circle_image"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="18dp"
                        android:src="@mipmap/ic_launcher"/>
                    <TextView
                        android:id="@+id/e_home_friend_circle_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/text_hei"
                        android:layout_toRightOf="@+id/e_home_friend_circle_image"
                        android:layout_marginLeft="20dp"
                        android:textSize="14dp"
                        android:text="@string/e_home_friend_circle_text_vstr"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#c6c6c6"
                        android:layout_toRightOf="@+id/e_home_friend_circle_image"
                        android:layout_below="@+id/e_home_friend_circle_text"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="4dp"
                        android:textSize="11dp"
                        android:text="订单"/>
                </RelativeLayout>
            </LinearLayout>
    <TextView
        style="@style/halving_line_e"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_margin="@dimen/left_right_retract_small">
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <ImageView
                        android:id="@+id/e_home_writer_image"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="18dp"
                        android:src="@mipmap/ic_launcher"/>
                    <TextView
                        android:id="@+id/e_home_writer_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/text_hei"
                        android:layout_toRightOf="@+id/e_home_writer_image"
                        android:layout_marginLeft="20dp"
                        android:textSize="14dp"
                        android:text="@string/e_home_writer_text_vstr"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#c6c6c6"
                        android:layout_toRightOf="@+id/e_home_writer_image"
                        android:layout_below="@+id/e_home_writer_text"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="4dp"
                        android:textSize="11dp"
                        android:text="订单"/>
                </RelativeLayout>
                <TextView
                    android:layout_width="1dp"
