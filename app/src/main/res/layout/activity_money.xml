<?xml version="1.0" encoding="utf-8"?>
    <com.handmark.pulltorefresh.library.PullToRefreshScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffffff"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="fill_parent"
            android:background="#E7E7E7"
            android:orientation="vertical"
            android:layout_height="wrap_content">
            <include layout="@layout/layout_money_top"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#E7E7E7"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/missed_ll"
                    android:padding="5dp"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:background="@drawable/select_click"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <!-- 已错过的个数 -->
                    <TextView
                        android:layout_marginTop="5dp"
                        android:id="@+id/tv_missed_number"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="#000000"
                        android:textSize="14dp" />
                    <TextView
                        android:layout_marginBottom="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/missed"
                        android:textSize="12dp" />




                </LinearLayout>

                <LinearLayout
                    android:id="@+id/earned_ll"
                    android:padding="5dp"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:background="@drawable/select_click"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <!-- 已赚取的金额 -->
                    <TextView
                        android:layout_marginTop="5dp"
                        android:id="@+id/tv_earned_number"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="#000000"
                        android:textSize="14dp" />

                    <TextView
                        android:layout_marginBottom="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/earned"
                        android:textSize="12dp" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/authstr_ll"
                    android:padding="5dp"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:background="@drawable/select_click"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <!-- 带审核的金额 -->
                    <TextView
                        android:layout_marginTop="5dp"
                        android:id="@+id/tv_authstr_number"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="#000000"
                        android:textSize="14dp" />
                    <TextView
                        android:layout_marginBottom="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/yetwithdraw"
                        android:textSize="12dp" />

                </LinearLayout>
                <LinearLayout
                    android:id="@+id/transfer_ll"
                    android:padding="5dp"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:background="@drawable/select_click"
                    android:gravity="center_vertical"
                    android:orientation="vertical">

                    <!-- 转账金额 -->
                    <TextView
                        android:layout_marginTop="5dp"
                        android:id="@+id/tv_transfer_number"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="#000000"
                        android:textSize="14dp" />
                    <TextView
                        android:layout_marginBottom="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/extra"
                        android:textSize="12dp" />

                </LinearLayout>
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="1px"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:layout_weight="1.5"
                android:background="#ffffff"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="14dp"
                    android:text="@string/earnings_yesterday"
                    android:textColor="#000000"
                    android:gravity="center_vertical"
                    android:textSize="16dp" />


                <!-- 昨日收益金额 -->
                <TextView
                    android:layout_marginLeft="5dp"
                    android:id="@+id/tv_earned_yesterday_num"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:text="0"
                    android:textColor="@color/orange"
                    android:textSize="18dp" />
                <TextView
                    android:id="@+id/tv_scorehelpurl_help"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginRight="20dp"
                    android:scaleType="center"
                    android:background="@mipmap/scorehelpurl"/>


            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#dcdcdc" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="4"
                android:orientation="vertical">

                <LinearLayout
                    android:weightSum="2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#fff"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <!-- 个人信息 -->
                    <RelativeLayout

                        android:id="@+id/layout_personInfo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/select_click"
                        android:layout_weight="1">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center_vertical">
                                <ImageView
                                    android:id="@+id/icon_personal"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="15dp"
                                    android:src="@mipmap/icon_personal" />
                            <LinearLayout
                                android:weightSum="2"
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TextView
                                    style="@style/halving_item_top"
                                    android:layout_weight="1"
                                    android:id="@+id/personal_info"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_toRightOf="@id/icon_personal"
                                    android:text="@string/money_info"
                                    android:textColor="#000000"
                                    android:textSize="15dp" />

                                <TextView
                                    style="@style/halving_item_bottom"
                                    android:layout_weight="1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"

                                    android:lines="2"
                                    android:text="@string/personal_info_perfect"
                                    android:textSize="10dp" />
                            </LinearLayout>
                        </LinearLayout>




                    </RelativeLayout>


                    <ImageView
                        android:layout_width="1px"
                        android:layout_height="match_parent"
                        android:background="#dcdcdc" />


                    <!-- 提现 -->
                    <RelativeLayout
                        android:id="@+id/layout_withdraw"

                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/select_click"
                        android:layout_weight="1">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center_vertical">
                                <ImageView
                                    android:id="@+id/icon_withdraw"
                                    android:layout_margin="15dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@mipmap/icon_withdraw" />
                            <LinearLayout
                                android:weightSum="2"
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TextView
                                    style="@style/halving_item_top"
                                    android:id="@+id/withdraw"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="@string/money_draw"
                                    android:textColor="#000000"
                                    android:textSize="15dp" />

                                <TextView
                                    style="@style/halving_item_bottom"
                                    android:layout_weight="1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignLeft="@id/withdraw"
                                    android:layout_below="@id/withdraw"
                                    android:text="@string/bindAccount"
                                    android:lines="2"
                                    android:textSize="10dp" />
                            </LinearLayout>
                        </LinearLayout>
                    </RelativeLayout>

                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:background="#dcdcdc" />

                <LinearLayout
                    android:weightSum="2"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="2"
                    android:background="#fff"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">


                    <!-- 榜单 -->
                    <RelativeLayout
                        android:id="@+id/layout_topN"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/select_click"
                        android:layout_weight="1">
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical">
                                <ImageView
                                    android:id="@+id/icon_topn"
                                    android:layout_margin="15dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@mipmap/icon_topn" />
                            <LinearLayout
                                android:weightSum="2"
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TextView
                                    style="@style/halving_item_top"
                                    android:layout_weight="1"
                                    android:id="@+id/topn"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"

                                    android:text="@string/money_topN"
                                    android:textColor="#000000"
                                    android:textSize="15dp" />

                                <TextView

                                    style="@style/halving_item_bottom"
                                    android:layout_weight="1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:text="@string/hard_work"
                                    android:lines="2"
                                    android:textSize="10dp" />
                            </LinearLayout>
                        </LinearLayout>



                    </RelativeLayout>


                    <ImageView
                        android:layout_width="1px"
                        android:layout_height="match_parent"
                        android:background="#dcdcdc" />


                    <!-- 个人纪录 -->
                    <RelativeLayout
                        android:id="@+id/layout_record"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/select_click"
                        android:layout_weight="1">


                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical">
                                <ImageView
                                    android:layout_margin="15dp"
                                    android:id="@+id/icon_record"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@mipmap/icon_record" />
                            <LinearLayout
                                android:weightSum="2"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TextView
                                    style="@style/halving_item_top"
                                    android:id="@+id/record"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_weight="1"
                                    android:text="@string/money_record"
                                    android:textColor="#000000"
                                    android:textSize="15dp" />

                                <TextView
                                    style="@style/halving_item_bottom"
                                    android:layout_weight="1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:lines="2"
                                    android:text="@string/record_recordedInfo"
                                    android:textSize="10dp" />
                            </LinearLayout>
                        </LinearLayout>

                    </RelativeLayout>


                </LinearLayout>
                <TextView
                    style="@style/halving_line_e"/>

            </LinearLayout>
            <TextView
                style="@style/halving_line"
                android:layout_height="10dp"
                android:background="#fafafa"/>
            <TextView
                style="@style/halving_line_e"/>
            <android.support.percent.PercentRelativeLayout
                android:id="@+id/layout_e"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_click"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:paddingBottom="15dp"
                android:paddingTop="15dp">

                <TextView
                    android:id="@+id/home"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:paddingLeft="10dp"
                    android:background="@mipmap/home" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/home"
                    android:text="@string/my_is_e"
                    android:layout_marginLeft="10dp"
                    android:textColor="#000000"
                    android:textSize="15dp" />
                <ImageView
                    android:id="@+id/ivh"
                    app:layout_widthPercent="2.5%"
                    app:layout_marginRightPercent="3%"
                    android:layout_centerVertical="true"
                    style="@style/into_image_style" />
            </android.support.percent.PercentRelativeLayout>
            <TextView
                style="@style/halving_line_e"/>
            <TextView
                style="@style/halving_line_e"
                android:layout_height="15dp"
                android:background="#fafafa"/>
            <TextView
                style="@style/halving_line_e"/>
            <!-- 分享 -->
            <android.support.percent.PercentRelativeLayout
                android:id="@+id/layout_share"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_click"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingBottom="15dp"
                android:paddingTop="15dp">
                <ImageView
                    android:id="@+id/ivs"
                    app:layout_widthPercent="2.5%"
                    app:layout_marginRightPercent="3%"
                    android:layout_centerVertical="true"
                    style="@style/into_image_style" />
                <TextView
                    android:id="@+id/icon_share"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:paddingLeft="10dp"
                    android:background="@mipmap/icon_share" />

                <TextView
                    android:id="@+id/tv_share"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/icon_share"
                    android:text="@string/share_app"
                    android:layout_marginLeft="10dp"
                    android:textColor="#000000"
                    android:textSize="15dp" />
                <ImageView
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/tv_share"
                    android:background="@mipmap/red_packet"
                    android:layout_width="25dp"
                    android:layout_height="20dp" />
            </android.support.percent.PercentRelativeLayout>
            <TextView
                style="@style/halving_line_e"/>
            <android.support.percent.PercentRelativeLayout
                android:id="@+id/layout_list_enter"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_click"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:paddingBottom="15dp"
                android:paddingTop="15dp">
                <ImageView
                    app:layout_widthPercent="2.5%"
                    app:layout_marginRightPercent="3%"
                    android:layout_centerVertical="true"
                    style="@style/into_image_style" />

                <TextView
                    android:id="@+id/tv_liste"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:paddingLeft="10dp"
                    android:background="@mipmap/icon_enter_list" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/tv_liste"
                    android:text="我关注的企业"
                    android:textColor="#000000"
                    android:textSize="15dp" />
            </android.support.percent.PercentRelativeLayout>
            <TextView
                style="@style/halving_line_e"/>
            <android.support.percent.PercentRelativeLayout
                android:id="@+id/layout_friend"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_click"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:paddingBottom="15dp"
                android:paddingTop="15dp">
                <ImageView
                    app:layout_widthPercent="2.5%"
                    app:layout_marginRightPercent="3%"
                    android:layout_centerVertical="true"
                    style="@style/into_image_style" />

                <TextView
                    android:id="@+id/f_money_friend"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:paddingLeft="10dp"
                    android:background="@mipmap/money_friend" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/f_money_friend"
                    android:text="@string/money_friend"
                    android:textColor="#000000"
                    android:textSize="15dp" />
            </android.support.percent.PercentRelativeLayout>
            <TextView
                style="@style/halving_line_e"/>


            <TextView
                style="@style/halving_line_e"
                android:layout_height="15dp"
                android:background="#fafafa"/>
            <TextView
                style="@style/halving_line_e"/>
            <android.support.percent.PercentRelativeLayout
                android:id="@+id/layout_yao"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_click"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:paddingBottom="15dp"
                android:paddingTop="15dp">
                <ImageView
                    android:id="@+id/ivyao"
                    app:layout_widthPercent="2.5%"
                    app:layout_marginRightPercent="3%"
                    android:layout_centerVertical="true"
                    style="@style/into_image_style" />
                <TextView
                    android:id="@+id/f_money_yao"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:paddingLeft="10dp"
                    android:background="@mipmap/activitys" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/f_money_yao"
                    android:text="活动"
                    android:textColor="#000000"
                    android:textSize="15dp" />
            </android.support.percent.PercentRelativeLayout>
            <TextView
                style="@style/halving_line_e"/>
            <android.support.percent.PercentRelativeLayout
                android:id="@+id/layout_youmi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_click"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:paddingBottom="15dp"
                android:paddingTop="15dp">
                <ImageView
                    android:id="@+id/iv_award"
                    app:layout_widthPercent="2.5%"
                    app:layout_marginRightPercent="3%"
                    android:layout_centerVertical="true"
                    style="@style/into_image_style" />
                <TextView
                    android:id="@+id/f_money_youmi"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:paddingLeft="10dp"
                    android:background="@mipmap/youmi" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/f_money_youmi"
                    android:text="有米奖励"
                    android:textColor="#000000"
                    android:textSize="15dp" />
            </android.support.percent.PercentRelativeLayout>
            <TextView
                style="@style/halving_line_e"/>
            <android.support.percent.PercentRelativeLayout
                android:id="@+id/layout_shop"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_click"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:paddingBottom="15dp"
                android:paddingTop="15dp">
                <ImageView
                    android:id="@+id/iv_shop"
                    app:layout_widthPercent="2.5%"
                    app:layout_marginRightPercent="3%"
                    android:layout_centerVertical="true"
                    style="@style/into_image_style" />
                <TextView
                    android:id="@+id/f_money_shop"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:paddingLeft="10dp"
                    android:background="@mipmap/conversion" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/f_money_shop"
                    android:text="积分兑换"
                    android:textColor="#000000"
                    android:textSize="15dp" />
            </android.support.percent.PercentRelativeLayout>
            <TextView
                style="@style/halving_line_e"/>
            <android.support.percent.PercentRelativeLayout
                android:id="@+id/layout_idea"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_click"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:paddingBottom="15dp"
                android:paddingTop="15dp">
                <ImageView
                    android:id="@+id/ivsu"
                    app:layout_widthPercent="2.5%"
                    app:layout_marginRightPercent="3%"
                    android:layout_centerVertical="true"
                    style="@style/into_image_style" />
                <TextView
                    android:id="@+id/f_money_su"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:paddingLeft="10dp"
                    android:background="@mipmap/su" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/f_money_su"
                    android:text="意见反馈"
                    android:textColor="#000000"
                    android:textSize="15dp" />
            </android.support.percent.PercentRelativeLayout>
            <TextView
                style="@style/halving_line_e"/>

            <android.support.percent.PercentRelativeLayout
                android:id="@+id/layout_set"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/select_click"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:paddingBottom="15dp"
                android:paddingTop="15dp">
                <ImageView
                    app:layout_widthPercent="2.5%"
                    app:layout_marginRightPercent="3%"
                    android:layout_centerVertical="true"
                    style="@style/into_image_style" />
                <TextView
                    android:id="@+id/f_money_set"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:paddingLeft="10dp"
                    android:background="@mipmap/money_set" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/f_money_set"
                    android:text="个人设置"
                    android:textColor="#000000"
                    android:textSize="15dp" />
            </android.support.percent.PercentRelativeLayout>
            <TextView
                style="@style/halving_line_e"/>
            <TextView
                android:layout_width="match_parent"
